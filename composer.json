{
    "name": "spyke113/ivzakaz-bot",
    "description": "Telegram bot with Laravel support package",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0",
        "nutgram/laravel": "^1.4.3",
        "openai-php/client": "^0.6.0",
        "guzzlehttp/guzzle": "^7.5",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}